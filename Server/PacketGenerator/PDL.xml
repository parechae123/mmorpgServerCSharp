<?xml version="1.0" encoding="utf-8" ?>
<!--PDL = Packet Definition List-->
<PDL>
	<packet name ="PlayerInfoReq">
		<long name="playerId"/>
		<string name="name"/>
		<!--계층적 구조의 예제-->
		<list name ="skill">
			<int name="id"/>
			<short name="level"/>
			<float name="duration"/>
		</list>
	</packet>
</PDL>
